a = float(input("Digite A:"))
b = float(input("Digite B:"))
c = float(input("Digite C:"))

import math

delta = (b**2) - 4 * a * c

if delta < 0:
		print("esta equação não possui raíses reais")

else :
		raiz1 = (-b + math.sqrt(delta)) / 2*a
		raiz2 = (-b - math.sqrt(delta)) / 2*a
		if delta == 0:			
				print("a raiz desta equação é",raiz1)
		elif delta > 0:
				if raiz1 > raiz2:
						aux = raiz1
						raiz1 = raiz2
						raiz2 = aux
				print("as raíses da equação são",raiz1"e",raiz2)
